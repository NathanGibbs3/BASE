		"wapmorgan/php-deprecation-detector": "^2.0",
		"squizlabs/php_codesniffer": "^3.7 || ^2.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0 || ^0.6.0",
		"phpcompatibility/php-compatibility": "^9.3"
	},
	"suggest": {
		"pear/mail_mime": ">=1.5.2 - Allows alerts to be sent as attachemnts.",
		"pear/mail": ">=1.4.1 - Allows sending of alerts via email."
